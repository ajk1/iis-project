<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
    <name>typeSystemDescriptor</name>
    <description>This typesystem only describes the input and output types of the CPE.
You need to implement the types requried for your ranking model.</description>
    <version>1.0</version>
    <vendor/>
  <types>
    <typeDescription>
      <name>type.ComponentAnnotation</name>
      <description>A parent annotation which expects individual annotation types to inherit from.</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>componentId</name>
          <description>An identifier that specifies which system produced this annotation.</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        </features>
    </typeDescription>
    <typeDescription>
      <name>type.InputDocument</name>
      <description>Stores all the questions and its associated answer candidates.</description>
      <supertypeName>type.ComponentAnnotation</supertypeName>
      <features>
        <featureDescription>
          <name>reviews</name>
          <description/>
          <rangeTypeName>uima.cas.FSList</rangeTypeName>
          <elementType>type.Review</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>type.Measurement</name>
      <description/>
      <supertypeName>uima.cas.TOP</supertypeName>
      <features>
        <featureDescription>
          <name>tp</name>
          <description>The number of true positives</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>fn</name>
          <description>The number of false negatives</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>fp</name>
          <description>The number of false positives</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>L1</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>L2</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  <typeDescription>
      <name>type.Review</name>
      <description/>
      <supertypeName>type.ComponentAnnotation</supertypeName>
      <features>
        <featureDescription>
          <name>reviewerId</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>rawText</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>tokens</name>
          <description/>
          <rangeTypeName>uima.cas.FSList</rangeTypeName>
          <elementType>type.Token</elementType>
        </featureDescription>
        <featureDescription>
          <name>sentences</name>
          <description/>
          <rangeTypeName>uima.cas.FSList</rangeTypeName>
          <elementType>type.Sentence</elementType>
        </featureDescription>
        <featureDescription>
          <name>score</name>
          <description/>
          <rangeTypeName>type.Score</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>bigrams</name>
          <description/>
          <rangeTypeName>uima.cas.FSList</rangeTypeName>
          <elementType>type.Ngram</elementType>
        </featureDescription>
        <featureDescription>
          <name>trigrams</name>
          <description/>
          <rangeTypeName>uima.cas.FSList</rangeTypeName>
          <elementType>type.Ngram</elementType>
        </featureDescription>
        <featureDescription>
          <name>reviewTime</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>summary</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>productId</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>helpfulness</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>helpfulnessTotal</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>unigrams</name>
          <description/>
          <rangeTypeName>uima.cas.FSList</rangeTypeName>
          <elementType>type.Ngram</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>type.Token</name>
      <description/>
      <supertypeName>type.ComponentAnnotation</supertypeName>
      <features>
        <featureDescription>
          <name>rawText</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>sentimentScore</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>negationScore</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>amplificationScore</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>reviewId</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>type.Score</name>
      <description/>
      <supertypeName>type.ComponentAnnotation</supertypeName>
      <features>
        <featureDescription>
          <name>regressionScores</name>
          <description/>
          <rangeTypeName>uima.cas.FloatList</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>classificationScores</name>
          <description/>
          <rangeTypeName>uima.cas.IntegerList</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>goldLabel</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>type.Sentence</name>
      <description/>
      <supertypeName>type.ComponentAnnotation</supertypeName>
      <features>
        <featureDescription>
          <name>rawText</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>tokens</name>
          <description/>
          <rangeTypeName>uima.cas.FSList</rangeTypeName>
          <elementType>type.Token</elementType>
        </featureDescription>
        <featureDescription>
          <name>bigrams</name>
          <description/>
          <rangeTypeName>uima.cas.FSList</rangeTypeName>
          <elementType>type.Ngram</elementType>
        </featureDescription>
        <featureDescription>
          <name>trigrams</name>
          <description/>
          <rangeTypeName>uima.cas.FSList</rangeTypeName>
          <elementType>type.Ngram</elementType>
        </featureDescription>
      <featureDescription>
          <name>unigrams</name>
          <description/>
          <rangeTypeName>uima.cas.FSList</rangeTypeName>
          <elementType>type.Ngram</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>type.Ngram</name>
      <description/>
      <supertypeName>type.ComponentAnnotation</supertypeName>
      <features>
        <featureDescription>
          <name>n</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>tokens</name>
          <description/>
          <rangeTypeName>uima.cas.FSList</rangeTypeName>
          <elementType>type.Token</elementType>
        </featureDescription>
      <featureDescription>
          <name>rawText</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>sentimentScore</name>
          <description/>
          <rangeTypeName>uima.cas.FloatList</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>negationScore</name>
          <description/>
          <rangeTypeName>uima.cas.FloatList</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>amplificationScore</name>
          <description/>
          <rangeTypeName>uima.cas.FloatList</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
